<% provide(:title, "Edit user") %>
<%= form_for(@member) do |f| %>
    <%= @member.inspect %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.label :full_name %>
    <%= f.text_field :full_name %>
    <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
<% end %>
